	<snippets>


		<snippet name="snippet_sb-label-set-container">
			<Panel class="sb-row__set-container">
				<Panel id="id-sb-row__sets">
					                               
				</Panel>
			</Panel>
		</snippet>


		                       

		<snippet name="snippet_sb-meta">
			<Panel class="sb-meta">
				<Panel class="top-bottom-flow full-width">
					<Panel class="full-width bottom-divider meta-row">
						<Image id="id-sb-meta__mode__image" textureheight="36" texturewidth="-1" src="file://{images}/icons/ui/casual.svg"/>
						                 
						<Label id="id-sb-meta__labels__mode-map" text="" />
						              
						<Panel class="time_container">
							<CSGOGameTimeLabel id="id-gametime__time" text="{s:s_gametime_time}" />
						</Panel>
					</Panel>
					<Panel class="full-width meta-row">
						                  
						<Panel id="id-sb-meta__musickit" class="hidden">
							<Image id="id-sb-meta__musickit-image" />
							<Label id="id-sb-meta__musickit-name" />
							<Button id="id-sb-meta__musickit-unborrow" class="hidden vertical-center" onactivate="$.DispatchEvent('Scoreboard_UnborrowMusicKit');" >
								<TooltipPanel tooltip="#tooltip_stopborrowmusickit" style='tooltip-position: bottom;'>
									<Image src="file://{images}/icons/ui/no_musickit.svg"/>
								</TooltipPanel>
							</Button>
						</Panel>

						                       
						<Label id="id-sb-meta__labels__server-viewers" class="" text="{s:server_name}{s:hltv_viewers}" />

						                     
						<Panel id="id-sb-meta__button-panel" class="hidden">

							                    
							<Panel class="sb-meta__button-panel__group">
								<TooltipPanel tooltip="#scoreboard_cyclestats_button_tooltip">
									<Button id="id-sb-meta__cycle" class="sb-meta__button-panel__btn hidden" onactivate="$.DispatchEvent('Scoreboard_CycleStats');">
										<Image src="file://{images}/icons/ui/stats.svg" textureheight='20' texturewidth='-1'/>
									</Button>
								</TooltipPanel>	
							</Panel>

							<Panel class="sb-meta__button-panel__group">
								<TooltipPanel tooltip="#scoreboard_mutevoice_button_tooltip">
									<Button id="id-sb-meta__mutevoice" class="sb-meta__button-panel__btn" onactivate="$.DispatchEvent('Scoreboard_MuteVoice');">
										<Image id="id-sb-meta__mutevoice__image" src="" textureheight='20' texturewidth='-1'/>
									</Button>
								</TooltipPanel>
							</Panel>

							<Panel class="sb-meta__button-panel__group">
								<TooltipPanel tooltip="#scoreboard_blockugc_button_tooltip">
									<Button id="id-sb-meta__blockugc" class="sb-meta__button-panel__btn" onactivate="$.DispatchEvent('Scoreboard_BlockUgc');">
										<Image id="id-sb-meta__blockugc__image" src="" textureheight='20' texturewidth='-1' />
									</Button>
								</TooltipPanel>
							</Panel>

						</Panel>
					</Panel>
				</Panel>

			</Panel>
		</snippet>



		                              

		<snippet name="snippet_scoreboard--no-teams">
			<Panel class="sb-main">

					                       
					<Frame snippet="snippet_sb-meta"/>

					               
					<Panel id="id-sb-players-table__labels-row" class="sb-row--labels sb-row no-hover" />

					                    
					<Panel class="sb-team__data sb-team__data--ANY" cachepaintcmdlist="true">
						<Panel id="players-table-ANY" class="sb-players-table" />
					</Panel>
					

				                   
				<Panel id="id-sb-team--Spectator" class="sb-team sb-team--Spectator" cachepaintcmdlist="true">
					<Panel class="sb-team__data sb-team__data--Spectator">
							<Panel id="players-table-Spectator" class="sb-players-table" />
					</Panel>
				</Panel>

				               
				<Frame snippet="snippet_sb-footer"/>

			</Panel>
		</snippet>


		                               

		<snippet name="snippet_scoreboard--cooperative">
			<Panel class="sb-main">

				                       
				<Frame snippet="snippet_sb-meta"/>

				               
				<Panel id="id-sb-players-table__labels-row" class="sb-row--labels sb-row no-hover" />

				                    
				<Panel class="sb-team__data sb-team__data--ANY" cachepaintcmdlist="true">
					<Panel id="players-table-ANY" class="sb-players-table" />
				</Panel>

				<Panel id="CoopStats" class="sb-coop-mission-info" >
					<Panel class="coop-mission-instructions">
						<Label html="true" text="#Quest_Guardian_Desc_op09" />
					</Panel>
					<Panel class="coop-mission-stats">
						<Label class="stats-header-previous" text="#SFUI_CoopSB_Rating_Prev"/>
						<Label class="stats-header-endgame" text="#SFUI_CoopSB_Rating"/>
						<Panel class="sb-coop-stats__row oddrow">
							<Label class="sb-coop-stat-name" text="#SFUI_Scoreboard_Coop_MissionCompletion"/>
							<Label class="sb-coop-stat-value" text=""/>
							<Label class="sb-coop-stat-score" text="10,000"/>
						</Panel>
						<Panel class="sb-coop-stats__row evenrow">
							<Label class="sb-coop-stat-name" text="{s:statname1}"/>
							<Label class="sb-coop-stat-value" text="{d:statval1}%"/>
							<Label class="sb-coop-stat-score" text="{d:statscore1}"/>
						</Panel>
						<Panel class="sb-coop-stats__row oddrow">
							<Label class="sb-coop-stat-name" text="{s:statname2}"/>
							<Label class="sb-coop-stat-value" text="{d:statval2}%"/>
							<Label class="sb-coop-stat-score" text="{d:statscore2}"/>
						</Panel>
						<Panel class="sb-coop-stats__row evenrow">
							<Label class="sb-coop-stat-name" text="{s:statname0}"/>
							<Label class="sb-coop-stat-value" text="{s:statval0}%"/>
							<Label class="sb-coop-stat-score" text="-{d:statscore0}"/>
						</Panel>
						<Panel class="sb-coop-stats__row oddrow">
							<Label class="sb-coop-stat-name" text="{s:statname3}"/>
							<Label class="sb-coop-stat-value" text="{d:statval3}"/>
							<Label class="sb-coop-stat-score" text="{d:statscore3}"/>
						</Panel>
						<Panel class="sb-coop-stats__row evenrow">
							<Label class="sb-coop-stat-name" text="{s:bonusname0}"/>
							<Label class="sb-coop-stat-value" text=""/>
							<Label class="sb-coop-stat-score" text="{d:bonusscore0}"/>
						</Panel>
						<Panel class="sb-coop-stats__row sb-coop-stats__total">
							<Label class="sb-coop-stat-name" text="#SFUI_CoopSB_Total"/>
							<Label class="sb-coop-stat-value" text=""/>                                
							<Label class="sb-coop-stat-score" text="{d:total_score}"/>
						</Panel>
					</Panel>
				</Panel>

				               
				<Frame snippet="snippet_sb-footer"/>

			</Panel>
		</snippet>

		
		                               

