<root>
	<styles>
		<include src="file://{resources}/styles/csgostyles.css" />
		<include src="file://{resources}/styles/radialradio.css" />
	</styles>

	<snippets>

		<snippet name="RadialRadioSegment">

			<Panel class="RadialRadioSegment">
				<Panel id="segment-data" class="RadialRadioSegment-data">
          <Panel id="item-image" class="RadialRadioSegment-container">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" />
            <Label id="item-text" class="RadialRadioSegment-text" text=""/>
          </Panel>
				</Panel>
			</Panel>

		</snippet>
    <snippet name="RadialRadioTab">

      <Panel class="RadialRadioTab">
        <Panel id="segment-data" class="RadialRadioSegment-data">
          <Label id="radio-tab-text" class="RadialRadioTab-text" text="{s:RadialRadioTab--name}"/>
        </Panel>
      </Panel>

    </snippet>
	</snippets>
	
	<CSGORadialRadio class="RadialRadio">
		<Panel id="backdrop" class="RadialRadioBackdrop">
      <Panel class="RadialRadioBackdropCircle"/>

			<Panel class="RadialRadioSegment MouseoverRadioTab--hover" style="clip: radial( 50.0% 50.0%, 22.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 50.0% 17.0% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Stick Together" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>



			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 67.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 73.33% 26.66% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Request weapon" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>



			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 112.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 83.33% 50.0% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Follow Me" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>


			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 157.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 73.33% 73.33% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Hold Position" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>

			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 202.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 50.00% 83.00% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Spread Out" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>


			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 247.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 26.66% 73.33% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Fall Back" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>


			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 292.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 17.0% 50.0% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="We should spend" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>

			<Panel class="RadialRadioSegment" style="clip: radial( 50.0% 50.0%, 337.48deg, 315.04deg);">
				<Panel id="segment-data" class="RadialRadioSegment-data" style="position: 26.665% 26.665% 0.0px;">
          <Panel id="item-image" class="RadialRadioSegment-container" style="position: 0.0px 0.0px 0.0px;">
					  <Image id="item-image-image" class="RadialRadioSegment-image" textureheight="64" scaling="none" style="visibility: visible; width: 64.0px; height: 64.0px; position: 0.0px 0.0px 0.0px;"/>
            <Label id="item-text" class="RadialRadioSegment-text" text="Who will lead?" style="position: 0.0px 64.0px 0.0px;"/>
          </Panel>
				</Panel>
			</Panel>


    </Panel>
    <Panel id="center-dead" class="RadialRadioCenterDot" force-no-composition-layer="true" style="visibility: collapse;"/>
		<Label id="item-selected-name" class="RadialRadioSegmentSelectedCenter-text" text="{s:Stick Together}"/>
	</CSGORadialRadio>
	


</root>

